{
  "name": "persone16/faker-russian-php",
  "description": "Russian faker for seed, for example on Laravel 12",
  "keywords": ["php", "faker", "package", "seed", "db"],
  "license": "MIT",
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "*",
    "squizlabs/php_codesniffer": "^3.13",
    "slevomat/coding-standard": "^8.20",
    "orchestra/testbench": "^10.4"
  },
  "autoload": {
    "psr-4": {
      "persone16\\FakerRussianPhp\\": "src/"
    }
  },
  "scripts": {
    "tests": "pest --stop-on-failure --colors",
    "tests-coverage": "pest --coverage --min=90",
    "tests-mutation": "pest --mutate",
    "lint": [
      "@phpstan",
      "@cs",
      "@rector"
    ],
    "phpstan": "phpstan analyse --memory-limit=2G",
    "rector": "rector",
    "cs": "phpcs"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
